CC=gcc
CFLAGS=/opt=2/warn#/debug/noopt/profile

libvms.olb: vmsdir.obj vmspwd.obj vmsfile.obj parse.obj getopt.obj getopt1.obj\
	    qsort.obj
	library/create libvms $^

.c.obj:
	$(CC) $(CFLAGS) /obj=$*.obj $*.c

fixsys:
	define/job sys sys$$library

unfixsys:
	deassign/job sys

parse.c vmsfile.c: parse.h
